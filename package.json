{
  "name": "vlh-forms",
  "version": "0.3.4",
  "description": "Forms Plugin for Learning House's new form system",
  "repository": {
    "url": "thelearninghouse/vlh-forms",
    "type": "git"
  },
  "main": "dist/vlh-forms.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "test-vue-app",
    "dev": "poi example/index.js",
